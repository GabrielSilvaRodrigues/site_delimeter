{
  "name": "site-delimeter",
  "version": "1.0.0",
  "description": "Site Delimeter with React frontend and PHP backend",
  "main": "src/index.js",
  "scripts": {
    "start": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "test": "cd frontend && npm test",
    "install-frontend": "cd frontend && npm install",
    "dev": "concurrently \"npm run start\" \"php -S localhost:8080 -t .\"",
    "build-prod": "cd frontend && npm run build && mkdir -p ../build && cp -r build/* ../build/",
    "serve": "php -S localhost:8080 -t .",
    "deploy": "npm run build-prod && npm run serve"
  },
  "dependencies": {
    "concurrently": "^7.6.0"
  },
  "devDependencies": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}